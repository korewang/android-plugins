<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.korewang.shuishui.activitys.GetGPSActivity" >
   
    <com.korewang.shuishui.widget.HeaderView
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/headerView" />
	 <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="sGetGPS"
            android:id="@+id/gps_btn"/>
    <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Hello World"
            android:id="@+id/getGps"
            />
    <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="wwwwwwww"
            android:id="@+id/txt_info"
            />

    <WebView
        android1:id="@+id/content"
        android1:layout_width="match_parent"
        android1:layout_height="match_parent" />

</LinearLayout>
